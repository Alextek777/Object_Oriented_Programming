[32m+[m[32mtemplate<typename contType>[m
[32m+[m[32mbool loadDataFile(contType& cont, std::string& fileName)		//2.1[m
[32m+[m[32m{[m
[32m+[m	[32mstd::ifstream str;[m
[32m+[m	[32mstr.open(fileName, std::ios::out | std::ios::app);[m
[32m+[m	[32mif (!str.is_open())[m
[32m+[m	[32m{[m
[32m+[m		[32mstd::cout << "failed opening file!" << std::endl;[m
[32m+[m		[32mreturn false;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mtypename contType::value_type dataT;[m
[32m+[m
[32m+[m	[32mwhile (!str.eof())[m
[32m+[m	[32m{[m
[32m+[m		[32mstr >> dataT;[m
[32m+[m		[32mcont.push_back(dataT);[m
[32m+[m	[32m}[m
[32m+[m	[32mcont.pop_back();[m
[32m+[m	[32mstr.close();[m
[32m+[m	[32mreturn true;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mtemplate<typename contType>[m
[32m+[m[32mbool loadDataIterator(contType& cont, std::string& fileName)		//2.2[m
[32m+[m[32m{[m
[32m+[m	[32mstd::ifstream fin;[m
[32m+[m[41m	[m
[32m+[m	[32mfin.open(fileName, std::ios::in);[m
[32m+[m	[32mif(!fin.is_open())[m
[32m+[m	[32m{[m
[32m+[m		[32mstd::cout << "couldn`t open file : " << fileName << std::endl;[m
[32m+[m		[32mreturn false;[m
[32m+[m	[32m}[m
[32m+[m	[32mstd::copy( std::istream_iterator<typename contType::value_type>(fin) , std::istream_iterator<typename contType::value_type>()  ,std::inserter(cont,cont.end()) );[m
[32m+[m	[32mfin.close();[m
[32m+[m	[32mreturn true;[m
[32m+[m[32m}[m

[32m+[m[32mvoid createData(std::string &FileName)[m
[32m+[m[32m{[m
[32m+[m	[32msrand(time(NULL));[m
[32m+[m	[32mstd::ofstream str;[m
[32m+[m	[32mstr.open(FileName, std::ios::out | std::ios::trunc);[m
[32m+[m
[32m+[m	[32mfor (int i = 0; i < 100; i++)[m
[32m+[m		[32mstr << rand() % 101 - 50 << " ";[m
[32m+[m	[32mstr.close();[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mvoid createData(std::string& FileName, std::vector<int>& vec)[m
[32m+[m[32m{[m
[32m+[m	[32mstd::generate(vec.begin(), vec.end(), []() {return rand() % 101 - 50; });[m
[32m+[m	[32mstd::ofstream out;[m
[32m+[m	[32mout.open(FileName, std::ios::out | std::ios::trunc);[m
[32m+[m
[32m+[m	[32mstd::copy(vec.begin(), vec.end(), std::ostream_iterator<int>(out," "));[m
[32m+[m	[32mout.close();[m
[32m+[m[32m}[m

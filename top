[32m+[m[32mtemplate<typename contType>[m
[32m+[m[32mvoid modify(contType& cont)	//3.1 #17[m
[32m+[m[32m{[m
[32m+[m	[32mtypename contType::value_type max;[m
[32m+[m	[32mtypename contType::value_type sum = 0;[m
[32m+[m	[32mmax = *cont.begin();[m
[32m+[m
[32m+[m	[32mfor (auto it = cont.begin(); it != cont.end(); ++it)[m
[32m+[m	[32m{[m
[32m+[m		[32mif (max < *it)	max = *it;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfor(auto it = cont.begin(); it != cont.end(); ++it)[m
[32m+[m	[32m{[m
[32m+[m		[32mif (*it > 0) *it = max;[m
[32m+[m		[32msum += abs(*it);[m
[32m+[m	[32m}[m
[32m+[m	[32mcont.push_back(sum / cont.size());[m
[32m+[m	[32mcont.push_back(sum);[m
[32m+[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mtemplate<typename iterType>[m
[32m+[m[32mvoid modify(iterType beg,iterType end)	//3.2 #17[m
[32m+[m[32m{[m
[32m+[m	[32mtypename iterType::value_type max = *beg;[m
[32m+[m	[32mfor (auto it = beg; it != end; it++)[m
[32m+[m	[32m{[m
[32m+[m		[32mif (max < *it)	max = *it;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfor (auto it = beg; it != end; it++)[m
[32m+[m	[32m{[m
[32m+[m		[32mif (*it > 0) *it = max;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mtemplate<typename iterType>[m
[32m+[m[32mvoid modify_each(iterType beg,iterType end)	//3.3 #17[m
[32m+[m[32m{[m
[32m+[m	[32mtypename iterType::value_type max = *beg;[m
[32m+[m	[32mfor (auto it = beg; it != end; it++)[m
[32m+[m	[32m{[m
[32m+[m		[32mif (max < *it)	max = *it;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mstd::for_each(beg, end, [=](typename iterType::value_type& current) {if (current > 0) current = max; });[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mtemplate<typename contType>[m
[32m+[m[32mvoid outputResultStream(contType& cont,std::string fileName)[m
[32m+[m[32m{[m
[32m+[m	[32mstd::ofstream fout;[m
[32m+[m	[32mfout.open(fileName, std::ios::out | std::ios::app);[m
[32m+[m	[32mif (!fout.is_open())[m
[32m+[m	[32m{[m
[32m+[m		[32mstd::cout << "couldn`t open file!" << std::endl;[m
[32m+[m		[32mreturn;[m
[32m+[m	[32m}[m
[32m+[m	[32mtypename contType::iterator iter = cont.begin();[m
[32m+[m	[32mwhile (iter != cont.end())[m
[32m+[m	[32m{[m
[32m+[m		[32mfout << *iter;[m
[32m+[m		[32m++iter;[m
[32m+[m	[32m}[m
[32m+[m	[32mfout.close();[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mtemplate<typename contType>[m
[32m+[m[32mvoid outputResultIterators(contType& cont, std::string fileName)[m
[32m+[m[32m{[m
[32m+[m	[32mstd::ofstream fout;[m
[32m+[m	[32mfout.open(fileName, std::ios::out | std::ios::app);[m
[32m+[m	[32mif (!fout.is_open())[m
[32m+[m	[32m{[m
[32m+[m		[32mstd::cout << "couldn`t open file!" << std::endl;[m
[32m+[m		[32mreturn;[m
[32m+[m	[32m}[m
[32m+[m	[32mstd::copy(cont.begin(), cont.end(), std::ostream_iterator<typename contType::value_type>(fout," "));[m
[32m+[m	[32mfout.close();[m
[32m+[m[32m}[m
[32m+[m
